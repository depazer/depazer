{
  "name": "@depazer/cli",
  "version": "0.1.1",
  "description": "A CLI tool for analyzing package dependencies",
  "type": "module",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "depazer": "./bin/cli.js"
  },
  "keywords": [
    "cli",
    "dependency",
    "analyzer"
  ],
  "bugs": {
    "url": "https://github.com/depazer/depazer/issues"
  },
  "homepage": "https://github.com/depazer/depazer",
  "packageManager": "pnpm@8.6.11",
  "engines": {
    "node": ">=14.17.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "registry": "https://github.com/depazer/depazer/tree/main/packages/cli",
  "scripts": {
    "prebuild": "rimraf ./lib",
    "build": "pnpm prebuild && tsx ./build/build.ts",
    "preview": "node ./bin/cli.js",
    "dev": "tsx ./src/main.ts",
    "test:dev": "vitest",
    "test": "vitest run",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "devDependencies": {
    "@depazer/core": "workspace:*",
    "@depazer/shared": "workspace:*",
    "@depazer/web": "workspace:*",
    "cac": "^6.7.14",
    "node-cache": "^5.1.2"
  }
}
