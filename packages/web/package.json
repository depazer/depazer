{
  "name": "@package-dependency-analyze/web",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test": "vitest run",
    "build-only": "vite build --emptyOutDir",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.1",
    "@unocss/reset": "^0.54.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.3.0",
    "d3": "^7.8.5",
    "idb-keyval": "^6.2.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.6",
    "unocss": "^0.54.0",
    "vite": "^4.4.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  }
}
