{
  "name": "@depazer/web",
  "version": "0.1.0",
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-cli build-vite",
    "preview": "vite preview --outDir dist/cli",
    "test": "vitest run",
    "build-vite": "vite build --base /__depazer --outDir dist/vite",
    "build-cli": "vite build --outDir dist/cli",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/"
  },
  "devDependencies": {
    "@depazer/core": "workspace: *",
    "@intlify/unplugin-vue-i18n": "^0.12.3",
    "@types/d3": "^7.4.0",
    "@types/jsdom": "^21.1.1",
    "@unocss/reset": "^0.55.2",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/core": "^10.3.0",
    "d3": "^7.8.5",
    "html2canvas": "^1.4.1",
    "idb-keyval": "^6.2.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.6",
    "unocss": "^0.55.2",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-i18n": "9.3.0-beta.26",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  }
}
