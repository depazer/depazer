{
  "$schema": "https://json.schemastore.org/package",
  "name": "dependency-analyzer",
  "type": "module",
  "version": "0.0.0",
  "description": "A CLI tool for analyzing package dependencies",
  "keywords": [
    "cli",
    "dependency",
    "analyzer"
  ],
  "bugs": {
    "url": "https://github.com/package-dependency-analyze/dependency-analyzer/issues"
  },
  "homepage": "https://github.com/package-dependency-analyze/dependency-analyzer",
  "files": [
    "./lib",
    "./bin"
  ],
  "packageManager": "pnpm@8.6.10",
  "engines": {
    "node": ">=14.17.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "depazer": "./bin/cli.js"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf ./lib",
    "build": "pnpm prebuild && turbo build --no-daemon",
    "preview": "node ./bin/cli.js",
    "commit": "cz && changeset add",
    "lint": "turbo lint --no-daemon",
    "test": "turbo test --no-daemon",
    "test:cov": "vitest run --coverage",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.json": "prettier --write"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/config-conventional": "^17.6.7",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^20.4.5",
    "@unocss/eslint-config": "^0.54.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.7",
    "cz-git": "^1.7.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "vitepress": "1.0.0-beta.7",
    "vitest": "^0.33.0"
  }
}